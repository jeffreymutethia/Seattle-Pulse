{
  "name": "seattle-pulse",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NEXT_PUBLIC_GIT_SHA=$(git rev-parse --short HEAD 2>/dev/null || echo 'unknown') next build",
    "build:local": "NEXT_PUBLIC_GIT_SHA=$(git rev-parse --short HEAD 2>/dev/null || echo 'local') next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "lint:fix": "next lint --fix",
    "test": "cypress run",
    "cypress:open": "cypress open",
    "cypress:open:electron": "cypress open --browser electron",
    "cypress:open:firefox": "cypress open --browser firefox",
    "cypress:run": "cypress run",
    "cypress:run:firefox": "cypress run --browser firefox",
    "cypress:run:electron": "cypress run --browser electron",
    "test:e2e": "start-server-and-test dev http://localhost:3000 cypress:run",
    "test:e2e:firefox": "start-server-and-test dev http://localhost:3000 cypress:run:firefox",
    "test:e2e:electron": "start-server-and-test dev http://localhost:3000 cypress:run:electron",
    "start:prod": "next start -p 3000",
    "perf": "npm run build && npm run start:prod"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@next/font": "^14.2.15",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.5",
    "@radix-ui/react-dropdown-menu": "^2.1.5",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.1.7",
    "@sentry/nextjs": "^10.25.0",
    "@types/react-select": "^5.0.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "debounce": "^2.2.0",
    "framer-motion": "^12.0.6",
    "js-cookie": "^3.0.5",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "lucide-react": "^0.469.0",
    "mixpanel-browser": "^2.72.0",
    "next": "^15.5.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^5.0.0",
    "react-hook-form": "^7.55.0",
    "react-hot-toast": "^2.5.2",
    "react-intersection-observer": "^9.15.1",
    "react-leaflet": "^5.0.0",
    "react-leaflet-markercluster": "^5.0.0-rc.0",
    "react-player": "^2.16.0",
    "react-select": "^5.10.1",
    "react-select-country-list": "^2.2.3",
    "socket.io-client": "^4.8.1",
    "sonner": "^1.7.3",
    "tailwind-merge": "^2.6.0",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/leaflet": "^1.9.16",
    "@types/lodash": "^4.17.15",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cypress": "^14.3.2",
    "eslint": "^8",
    "eslint-config-next": "15.0.4",
    "postcss": "^8",
    "start-server-and-test": "^2.0.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "browserslist": {
    "production": [
      "Chrome >= 110",
      "Firefox >= 110",
      "Safari >= 16",
      "iOS >= 16",
      "Edge >= 110",
      "not IE 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
